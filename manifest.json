{
    "manifest_version": 3,
    "name": "Comic Sansifier",
    "description": "The worst extension you will ever own, and will only use a couple of times!",
    "version": "1.0",
    "action": {
        "default_icon": "/images/cs16.png"
    },
    "permissions": [
        "scripting"
    ],
    "background": {
        "service_worker": "/main/main.js"
    },
    "icons": {
        "16": "/images/cs16.png",
        "32": "/images/cs32.png",
        "48": "/images/cs48.png",
        "128": "/images/cs128.png"
    }
}